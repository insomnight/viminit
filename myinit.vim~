" vim-plug setting file: plugins.vim
let $VIMINIT = 'c:\vimini'
source $VIMINIT/plugins.vim

" vim setting file: configs.vim
source $VIMINIT/configs.vim

" vim setting function file: functions.vim
source $VIMINIT/functions.vim

" keyborad mapping config file: mappings.vim
source $VIMINIT/mappings.vim
